      <drawSize>(1, 3)</drawSize>
      <shadowData>
        <volume>(0.9, 0.4, 3)</volume>
      </shadowData>
    </graphicData>
    <size>(1, 3)</size>
    <fillPercent>0.40</fillPercent>
    <altitudeLayer>Building</altitudeLayer>
    <statBases>
      <MaxHitPoints>250</MaxHitPoints>
      <WorkToBuild>1500</WorkToBuild>
      <Flammability>0.5</Flammability>
    </statBases>
    <passability>PassThroughOnly</passability>
    <pathCost>50</pathCost>
    <building>
      <canLandGravshipOn>true</canLandGravshipOn>
      <isInert>true</isInert>
    </building>
  </ThingDef>

  <ThingDef ParentName="BuildingBase">
    <defName>AncientBarrel</defName>
    <label>ancient barrel</label>
    <description>An ancient, broken barrel that looks like it once contained waste of some sort. The only thing it contains now is a smear of dried sludge.</description>
    <rotatable>false</rotatable>
    <noRightClickDraftAttack>true</noRightClickDraftAttack>
    <graphicData>
      <texPath>Things/Building/Ruins/AncientBarrel</texPath>
      <graphicClass>Graphic_Random</graphicClass>
      <drawSize>(1,1)</drawSize>
      <shadowData>
        <volume>(0.5, 0.5, 0.5)</volume>
      </shadowData>
    </graphicData>
    <size>(1, 1)</size>
    <fillPercent>0.30</fillPercent>
    <altitudeLayer>Building</altitudeLayer>
    <statBases>
      <MaxHitPoints>50</MaxHitPoints>
      <WorkToBuild>200</WorkToBuild>
      <Flammability>0.7</Flammability>
    </statBases>
    <passability>PassThroughOnly</passability>
    <pathCost>50</pathCost>
    <building>
