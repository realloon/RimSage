<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <TerrainDef>
    <defName>Underwall</defName>
    <label>underwall</label>
    <texturePath>Terrain/Surfaces/Underwall</texturePath>
    <edgeType>Hard</edgeType>
    <renderPrecedence>0</renderPrecedence>
    <fertility>0</fertility>
  </TerrainDef>

  <TerrainDef ParentName="FloorBase">
    <defName>AncientTile</defName>
    <label>ancient tile</label>
    <description>Ancient metal tiles.</description>
    <statBases>
      <MarketValue>0</MarketValue>
      <Beauty>0</Beauty>
      <Cleanliness>0.2</Cleanliness>
      <CleaningTimeFactor>0.6</CleaningTimeFactor>
    </statBases>
    <renderPrecedence>240</renderPrecedence>
    <texturePath>Terrain/Surfaces/AncientTile</texturePath>
    <isPaintable>true</isPaintable>
    <designationCategory Inherit="False" IsNull="True" />
  </TerrainDef>

  <TerrainDef ParentName="FloorBase">
    <defName>AncientConcrete</defName>
    <label>ancient concrete</label>
    <description>Ancient concrete flooring.</description>
    <statBases>
      <MarketValue>0</MarketValue>
      <Beauty>-1</Beauty>
      <CleaningTimeFactor>0.8</CleaningTimeFactor>
    </statBases>
    <renderPrecedence>70</renderPrecedence>
    <texturePath>Terrain/Surfaces/AncientConcrete</texturePath>
    <color>(140,140,140)</color>
    <isPaintable>true</isPaintable>
    <edgeType>Hard</edgeType>
    <designationCategory Inherit="False" IsNull="True" />
  </TerrainDef>

  <TerrainDef  ParentName="FloorBase">
    <defName>AncientWoodPlankFloor</defName>
    <label>ancient wood floor</label>
    <renderPrecedence>250</renderPrecedence>
    <texturePath>Terrain/Surfaces/WoodFloor</texturePath>
    <burnedDef>BurnedWoodPlankFloor</burnedDef>
    <color>(92,65,44)</color>
    <pollutionShaderType MayRequire="Ludeon.RimWorld.Biotech">TerrainFadeRoughSoftLight</pollutionShaderType>
    <pollutionOverlayTexturePath>Terrain/Surfaces/PollutionFloorSmooth</pollutionOverlayTexturePath>
    <isPaintable>true</isPaintable>
    <statBases>
      <MarketValue>0</MarketValue>
      <Flammability>0.22</Flammability>
    </statBases>
    <designationCategory Inherit="False" IsNull="True" />
  </TerrainDef>

</Defs>
