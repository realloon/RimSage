            </li>
          </parts>
        </li>
        <li>
          <def>Leg</def>
          <customLabel>right forelimb</customLabel>
          <coverage>0.03</coverage>
          <height>Bottom</height>
          <parts>
            <li>
              <def>Paw</def>
              <customLabel>front right flipper</customLabel>
              <coverage>0.5</coverage>
              <groups>
                <li>FrontRightPaw</li>
              </groups>
            </li>
          </parts>
        </li>
        <li>
          <def>Leg</def>
          <customLabel>left hindlimb</customLabel>
          <coverage>0.05</coverage>
          <height>Bottom</height>
          <parts>
            <li>
              <def>Paw</def>
              <customLabel>rear left flipper</customLabel>
              <coverage>0.5</coverage>
            </li>
          </parts>
        </li>
