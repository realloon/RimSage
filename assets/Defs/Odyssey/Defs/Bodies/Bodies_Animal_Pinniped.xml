                  <groups>
                    <li>Teeth</li>
                  </groups>
                </li>
              </parts>
            </li>
          </parts>
        </li>
        <li>
          <def>Leg</def>
          <customLabel>left forelimb</customLabel>
          <coverage>0.03</coverage>
          <height>Bottom</height>
          <parts>
            <li>
              <def>Paw</def>
              <customLabel>front left flipper</customLabel>
              <coverage>0.5</coverage>
              <groups>
                <li>FrontLeftPaw</li>
