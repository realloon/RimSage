  <SoundDef>
    <defName>Gravship_Engine_Start</defName>  
    <maxSimultaneous>1</maxSimultaneous>  
    <subSounds>
      <li>
        <onCamera>True</onCamera>      
        <grains>
          <li Class="AudioGrain_Clip">
            <clipPath>Gravship/Gravship_Engine_Start</clipPath>
          </li>
        </grains>      
        <volumeRange>80.0</volumeRange>
      </li>
    </subSounds>
  </SoundDef>

