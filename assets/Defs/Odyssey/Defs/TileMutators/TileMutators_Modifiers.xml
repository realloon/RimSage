<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <TileMutatorDef>
    <defName>AnimalLife_Increased</defName>
    <label>increased animal life</label>
    <description>Animals are more abundant here.</description>
    <categories>
      <li>AnimalLife</li>
    </categories>
    <chanceOnNonLandmarkTile>0.02</chanceOnNonLandmarkTile>
    <animalDensityFactor>1.5</animalDensityFactor>
    <animalDensityRange>0.3~4</animalDensityRange>
    <pollutionRange>0~0.25</pollutionRange>
  </TileMutatorDef>

  <TileMutatorDef>
    <defName>AnimalLife_Decreased</defName>
    <label>decreased animal life</label>
    <description>Animals are less abundant here than normal.</description>
    <categories>
      <li>AnimalLife</li>
    </categories>
    <chanceOnNonLandmarkTile>0.01</chanceOnNonLandmarkTile>
    <animalDensityFactor>0.5</animalDensityFactor>
  </TileMutatorDef>

  <TileMutatorDef>
    <defName>AnimalHabitat</defName>
    <label>animal habitat</label> <!-- not actually visible, overriden with a translation -->
    <description>A natural habitat for {ANIMALKIND_labelPlural}. They appear in greater numbers here.</description>
    <workerClass>TileMutatorWorker_AnimalHabitat</workerClass>
    <chanceOnNonLandmarkTile>0.02</chanceOnNonLandmarkTile>
    <animalDensityRange>0.3~7</animalDensityRange>
    <pollutionRange>0~0.25</pollutionRange>
  </TileMutatorDef>

  <TileMutatorDef>
    <defName>PlantLife_Increased</defName>
    <label>increased plant life</label>
    <description>Plant life is more abundant here.</description>
    <categories>
      <li>PlantLife</li>
    </categories>
    <chanceOnNonLandmarkTile>0.02</chanceOnNonLandmarkTile>
    <plantDensityFactor>1.3</plantDensityFactor>
    <plantDensityRange>0.0001~0.7</plantDensityRange>
    <pollutionRange>0~0.25</pollutionRange>
    <biomeBlacklist>
      <li>IceSheet</li>
      <li>SeaIce</li>
    </biomeBlacklist>
  </TileMutatorDef>

  <TileMutatorDef>
    <defName>PlantLife_Decreased</defName>
    <label>decreased plant life</label>
    <description>Plant life is less abundant here.</description>
    <categories>
      <li>PlantLife</li>
    </categories>
    <chanceOnNonLandmarkTile>0.01</chanceOnNonLandmarkTile>
    <plantDensityFactor>0.5</plantDensityFactor>
    <plantDensityRange>0.3~4</plantDensityRange>
  </TileMutatorDef>
  
  <TileMutatorDef>
    <defName>PlantGrove</defName>
    <label>plant grove</label> <!-- not actually visible, overriden with a translation -->
    <description>This area contains a large number of {0}s.</description>
    <workerClass>TileMutatorWorker_PlantGrove</workerClass>
    <categories>
      <li>WildPlants</li>
    </categories>
    <chanceOnNonLandmarkTile>0.02</chanceOnNonLandmarkTile>
    <plantDensityRange>0.3~4</plantDensityRange>
    <pollutionRange>0~0.25</pollutionRange>
    <biomeWhitelist>
      <li>TemperateForest</li>
      <li>TemperateSwamp</li>
      <li>ColdBog</li>
      <li>BorealForest</li>
      <li>TropicalSwamp</li>
      <li>TropicalRainforest</li>
      <li>Glowforest</li>
    </biomeWhitelist>
    <plantKinds>
      <li>Plant_TreeOak</li>
      <li>Plant_TreePoplar</li>
      <li>Plant_TreePine</li>
      <li>Plant_TreeBirch</li>
      <li>Plant_TreeWillow</li>
      <li>Plant_TreeMaple</li>
      <li>Plant_TreeCecropia</li>
      <li>Plant_Timbershroom</li>
      <li>Plant_Willowgill</li>
      <li>Boomshroom</li>
    </plantKinds>
  </TileMutatorDef>

  <TileMutatorDef Name="WildPlants_Base" Abstract="True">
    <label>wild plants</label> <!-- not actually visible, overriden with a translation -->
    <workerClass>TileMutatorWorker_WildPlants</workerClass>
    <categories>
      <li>WildPlants</li>
    </categories>
    <plantDensityRange>0.3~4</plantDensityRange>
    <pollutionRange>0~0.25</pollutionRange>
  </TileMutatorDef>

  <TileMutatorDef ParentName="WildPlants_Base">
    <defName>WildPlants</defName>
    <description>When warm enough, this area can grow a large number of {0}.</description>
    <workerClass>TileMutatorWorker_WildPlants</workerClass>
    <chanceOnNonLandmarkTile>0.03</chanceOnNonLandmarkTile>
    <biomeWhitelist>
      <li>AridShrubland</li>
      <li>BorealForest</li>
      <li>ColdBog</li>
      <li>GlacialPlain</li>
      <li>Grasslands</li>
      <li>LavaField</li>
      <li>TemperateForest</li>
      <li>TemperateSwamp</li>
      <li>TropicalRainforest</li>
      <li>TropicalSwamp</li>
      <li>Tundra</li>
    </biomeWhitelist>
    <plantKinds>
      <li>Plant_Cotton_Wild</li>
      <li>Plant_Psychoid_Wild</li>
      <li>Plant_Smokeleaf_Wild</li>
      <li>Plant_Tinctoria_Wild</li>
      <li>Plant_Strawberry_Wild</li>
      <li MayRequire="Ludeon.RimWorld.Ideology">Plant_Fibercorn_Wild</li>
    </plantKinds>
  </TileMutatorDef>
  
  <TileMutatorDef ParentName="WildPlants_Base">
    <defName>WildTropicalPlants</defName>
    <description>This area can grow a large number of {0}s.</description>
    <workerClass>TileMutatorWorker_WildPlants</workerClass>
    <chanceOnNonLandmarkTile>0.01</chanceOnNonLandmarkTile>
    <biomeWhitelist>
      <li>TropicalRainforest</li>
      <li>TropicalSwamp</li>
    </biomeWhitelist>
    <plantKinds>
      <li>Plant_TreeCocoa_Wild</li>
    </plantKinds>
  </TileMutatorDef>

  <TileMutatorDef>
    <defName>Junkyard</defName>
    <label>junkyard</label>
    <description>An old dumping ground for various kinds of junk.</description>
    <chanceOnNonLandmarkTile>0.01</chanceOnNonLandmarkTile>
    <junkDensityFactor>15</junkDensityFactor>
    <extraGenSteps>
      <li>ScarlandsJunkClusters</li>
      <li>ScarlandsJunkPrefabs</li>
    </extraGenSteps>
    <maxHilliness>SmallHills</maxHilliness>
  </TileMutatorDef>

  <TileMutatorDef>
    <defName>SteamGeysers_Increased</defName>
    <label>high geothermal activity</label>
    <description>Steam geysers are more common in this area.</description>
    <chanceOnNonLandmarkTile>0.01</chanceOnNonLandmarkTile>
    <geyserCountFactor>2</geyserCountFactor>
    <biomeBlacklist>
      <li>Grasslands</li>
      <li>GlacialPlain</li>
    </biomeBlacklist>
  </TileMutatorDef>

  <TileMutatorDef>
    <defName>MineralRich</defName>
    <label>mineral rich</label> <!-- not actually visible, overriden with a translation -->
    <workerClass>TileMutatorWorker_MineralRich</workerClass>
    <resourceBlacklist>
      <li>MineableObsidian</li>
    </resourceBlacklist>
    <biomeBlacklist>
      <li>IceSheet</li>
      <li>SeaIce</li>
    </biomeBlacklist>
  </TileMutatorDef>

  <TileMutatorDef>
    <defName>ArcheanTrees</defName>
    <label>archean trees</label>
    <description>This area contains ancient archean trees. These trees host a special microbial community in their roots which converts nearby terrain into rich soil. Their delicate root system makes it impossible to replant them.</description>
    <workerClass>TileMutatorWorker_ArcheanTrees</workerClass>
    <pollutionRange>0~0.25</pollutionRange>
    <biomeWhitelist>
      <li>AridShrubland</li>
      <li>BorealForest</li>
      <li>ColdBog</li>
      <li>GlacialPlain</li>
      <li>Grasslands</li>
      <li>LavaField</li>
      <li>TemperateForest</li>
      <li>TemperateSwamp</li>
      <li>TropicalRainforest</li>
      <li>TropicalSwamp</li>
      <li>Tundra</li>
    </biomeWhitelist>
  </TileMutatorDef>

  <!-- Weather Variants -->

  <TileMutatorDef>
    <defName>FoggyMutator</defName>
    <label>foggy</label>
    <description>Fog is common in this area.</description>
    <categories>
      <li>Weather</li>
    </categories>
    <workerClass>TileMutatorWorker_IncreaseWeatherFrequency</workerClass>
    <chanceOnNonLandmarkTile>0.01</chanceOnNonLandmarkTile>
    <weathersToAffect>
      <li>Fog</li>
    </weathersToAffect>
    <weatherFrequencyOffset>10</weatherFrequencyOffset>
    <biomeWhitelist>
      <li>BorealForest</li>
      <li>ColdBog</li>
      <li>TemperateForest</li>
      <li>TemperateSwamp</li>
      <li>TropicalRainforest</li>
      <li>TropicalSwamp</li>
      <li>Grasslands</li>
      <li>Glowforest</li>
      <li>Scarlands</li>
    </biomeWhitelist>
  </TileMutatorDef>

  <TileMutatorDef>
    <defName>SunnyMutator</defName>
    <label>sunny</label>
    <description>Clear weather is much more common here.</description>
    <categories>
      <li>Weather</li>
    </categories>
    <workerClass>TileMutatorWorker_IncreaseWeatherFrequency</workerClass>
    <chanceOnNonLandmarkTile>0.02</chanceOnNonLandmarkTile>
    <weathersToAffect>
      <li>Clear</li>
    </weathersToAffect>
    <weatherFrequencyOffset>10</weatherFrequencyOffset>
    <biomeBlacklist>
      <li>Glowforest</li>
    </biomeBlacklist>
  </TileMutatorDef>

  <TileMutatorDef>
    <defName>WetClimate</defName>
    <label>wet climate</label>
    <description>Precipitation is much more common here.</description>
    <categories>
      <li>Weather</li>
    </categories>
    <workerClass>TileMutatorWorker_IncreaseWeatherFrequency</workerClass>
    <chanceOnNonLandmarkTile>0.01</chanceOnNonLandmarkTile>
    <weathersToAffect>
      <li>Rain</li>
      <li>RainyThunderstorm</li>
      <li>SnowGentle</li>
      <li>SnowHard</li>
      <li>Blizzard</li>
      <li>FoggyRain</li>
    </weathersToAffect>
    <weatherFrequencyOffset>10</weatherFrequencyOffset>
    <biomeWhitelist>
      <li>BorealForest</li>
      <li>ColdBog</li>
      <li>IceSheet</li>
      <li>SeaIce</li>
      <li>TemperateForest</li>
      <li>TemperateSwamp</li>
      <li>TropicalRainforest</li>
      <li>TropicalSwamp</li>
      <li>Glowforest</li>
    </biomeWhitelist>
  </TileMutatorDef>

  <TileMutatorDef>
    <defName>WindyMutator</defName>
    <label>windy</label>
    <description>Winds are much stronger in this area.</description>
    <categories>
      <li>Weather</li>
    </categories>
    <workerClass>TileMutatorWorker_IncreaseWeatherFrequency</workerClass>
    <chanceOnNonLandmarkTile>0.01</chanceOnNonLandmarkTile>
    <weathersToAffect>
      <li>Windy</li>
    </weathersToAffect>
    <weatherFrequencyOffset>10</weatherFrequencyOffset>
    <additionalGameConditions>
      <li>WindyGameCondition</li>
    </additionalGameConditions>
    <biomeWhitelist>
      <li>AridShrubland</li>
      <li>Desert</li>
      <li>ExtremeDesert</li>
      <li>IceSheet</li>
      <li>SeaIce</li>
      <li>Tundra</li>
      <li>Grasslands</li>
      <li>GlacialPlain</li>
      <li>LavaField</li>
      <li>Scarlands</li>
    </biomeWhitelist>
  </TileMutatorDef>

  <TileMutatorDef MayRequire="Ludeon.RimWorld.Biotech">
    <defName>Pollution_Increased</defName>
    <label>increased pollution</label>
    <description>This area is contaminated with toxic pollutants.</description>
    <workerClass>TileMutatorWorker_IncreasedPollution</workerClass>
    <categories>
      <li>Pollution</li>
    </categories>
  </TileMutatorDef>

  <TileMutatorDef>
    <defName>Fish_Increased</defName>
    <label>increased fish</label>
    <description>This area has more fish than normal.</description>
    <categories>
      <li>Fish</li>
    </categories>
    <chanceOnNonLandmarkTile>0</chanceOnNonLandmarkTile>
    <fishPopulationFactor>1.5</fishPopulationFactor>
    <pollutionRange>0~0.25</pollutionRange>
    <biomeBlacklist>
      <li>Scarlands</li>
      <li>Desert</li>
      <li>ExtremeDesert</li>
    </biomeBlacklist>
  </TileMutatorDef>
  
  <TileMutatorDef>
    <defName>Fish_Decreased</defName>
    <label>decreased fish</label>
    <description>This area has fewer fish than normal.</description>
    <categories>
      <li>Fish</li>
    </categories>
    <chanceOnNonLandmarkTile>0</chanceOnNonLandmarkTile>
    <fishPopulationFactor>0.35</fishPopulationFactor>
    <biomeBlacklist>
      <li>Scarlands</li>
    </biomeBlacklist>
  </TileMutatorDef>

  <TileMutatorDef>
    <defName>DryGround</defName>
    <label>dry ground</label>
    <description>The ground in this area is drier than normal. There are no small ponds or bodies of water.</description>
    <categories>
      <li>Groundwater</li>
    </categories>
    <chanceOnNonLandmarkTile>0.02</chanceOnNonLandmarkTile>
    <preventsPondGeneration>true</preventsPondGeneration>
    <biomeWhitelist>
      <li>Scarlands</li>
      <li>BorealForest</li>
      <li>Tundra</li>
    </biomeWhitelist>
  </TileMutatorDef>

  <TileMutatorDef>
    <defName>ObsidianDeposits</defName>
    <label>obsidian deposits</label>
    <description>This area has deposits of volcanic obsidian, which can be mined for obsidian glass.</description>
    <chanceOnNonLandmarkTile>0.05</chanceOnNonLandmarkTile>
    <workerClass>TileMutatorWorker_ObsidianDeposits</workerClass>
    <biomeWhitelist>
      <li>LavaField</li>
    </biomeWhitelist>
  </TileMutatorDef>

</Defs>