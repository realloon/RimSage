    <chemicalAddictionChance>0.06</chemicalAddictionChance>
    <apparelColor>(255,255,255)</apparelColor>
    <apparelTags>
      <li>Vacsuit</li>
    </apparelTags>
    <ignoreApparelAllowChance>true</ignoreApparelAllowChance>
    <apparelAllowHeadgearChance>1</apparelAllowHeadgearChance>
    <specificApparelRequirements>
      <li>
        <bodyPartGroup>FullHead</bodyPartGroup>
        <requiredTag>Vacsuit</requiredTag>
