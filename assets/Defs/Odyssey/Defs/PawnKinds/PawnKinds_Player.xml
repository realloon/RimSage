    <techHediffsMoney>50~800</techHediffsMoney>
    <techHediffsTags>
      <li>Poor</li>
      <li>Simple</li>
      <li>ImplantEmpireCommon</li>
    </techHediffsTags>
    <techHediffsDisallowTags>
      <li>PainCauser</li>
    </techHediffsDisallowTags>
    <initialResistanceRange>13~21</initialResistanceRange>
  </PawnKindDef>
