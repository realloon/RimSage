<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <BiomeDef>
    <defName>LavaField</defName>
    <label>lava field</label>
    <description>Barren wastes burned clean by volcanic heat. Periodic lava eruptions can burn people and buildings, but also enrich the soil with mineral deposits.</description>
    <workerClass>BiomeWorker_LavaField</workerClass>
    <settleWarning>This area has high volcanic activity. It is threatened by clouds of volcanic ash, as well as periodic lava eruptions.</settleWarning>
    <allowFarmingCamps>false</allowFarmingCamps>
    <animalDensity>2.2</animalDensity>
    <plantDensity>0.45</plantDensity>
    <settlementSelectionWeight>0.1</settlementSelectionWeight>
    <movementDifficulty>1.5</movementDifficulty>
    <lakeBeachTerrain>Gravel</lakeBeachTerrain>
    <riverbankTerrain>Riverbank</riverbankTerrain>
    <riverbankSizeRange>6~8</riverbankSizeRange>
    <texture>World/Biomes/LavaFields</texture>
    <forageability>0.5</forageability>
    <foragedFood>RawAgave</foragedFood>
    <wildPlantRegrowDays>27</wildPlantRegrowDays>
    <geyserCountFactor>1.5</geyserCountFactor>
    <maxFishPopulation>480</maxFishPopulation>
    <fishTypes>
      <freshwater_Common>
        <Fish_Bass>1</Fish_Bass>
      </freshwater_Common>
      <freshwater_Uncommon>
        <Fish_Catfish>1</Fish_Catfish>
      </freshwater_Uncommon>
      <saltwater_Common>
        <Fish_Bluefish>1</Fish_Bluefish>
      </saltwater_Common>
      <saltwater_Uncommon>
        <Fish_Flounder>1</Fish_Flounder>
        <Fish_Marlin>1</Fish_Marlin>
      </saltwater_Uncommon>
      <rareCatchesSetMaker>RareFishingCatches_Hot</rareCatchesSetMaker>
    </fishTypes>
    <soundsAmbient>
      <li>Ambient_NightInsects_Standard</li>
    </soundsAmbient>
    <diseaseMtbDays>65</diseaseMtbDays>
    <diseases>
      <li>
        <diseaseInc>Disease_Flu</diseaseInc>
        <commonality>100</commonality>
      </li>
      <li>
        <diseaseInc>Disease_Plague</diseaseInc>
        <commonality>100</commonality>
      </li>
      <li>
        <diseaseInc>Disease_GutWorms</diseaseInc>
        <commonality>60</commonality>
      </li>
      <li>
        <diseaseInc>Disease_MuscleParasites</diseaseInc>
        <commonality>60</commonality>
      </li>
      <li>
        <diseaseInc>Disease_FibrousMechanites</diseaseInc>
        <commonality>30</commonality>
      </li>

      <li>
        <diseaseInc>Disease_SensoryMechanites</diseaseInc>
        <commonality>30</commonality>
      </li>
      <li>
        <diseaseInc>Disease_AnimalFlu</diseaseInc>
        <commonality>100</commonality>
      </li>
      <li>
        <diseaseInc>Disease_AnimalPlague</diseaseInc>
        <commonality>100</commonality>
      </li>
      <li>
        <diseaseInc>Disease_OrganDecay</diseaseInc>
        <commonality>10</commonality>
      </li>
    </diseases>
    <terrainsByFertility>
      <li>
        <terrain>VolcanicRock</terrain>
        <min>-999</min>
        <max>0.4</max>
      </li>
      <li>
        <terrain>Soil</terrain>
        <min>0.4</min>
        <max>0.8</max>
      </li>
      <li>
        <terrain>SoilRich</terrain>
        <min>0.8</min>
        <max>999</max>
      </li>
    </terrainsByFertility>
    <terrainPatchMakers>
      <li>
        <perlinFrequency>0.035</perlinFrequency>
        <perlinLacunarity>1.5</perlinLacunarity>
        <perlinOctaves>6</perlinOctaves>
        <isPond>true</isPond>
        <thresholds>
          <li>
            <terrain>VolcanicRock</terrain>
            <min>0.6</min>
            <max>0.9</max>
          </li>
          <li>
            <terrain>LavaDeep</terrain>
            <min>0.9</min>
            <max>999</max>
          </li>
        </thresholds>
      </li>
    </terrainPatchMakers>
    <baseWeatherCommonalities>
      <Clear>18</Clear>
      <Fog>1</Fog>
      <Rain>2</Rain>
      <DryThunderstorm>1</DryThunderstorm>
      <RainyThunderstorm>1</RainyThunderstorm>
      <FoggyRain>0.5</FoggyRain>
      <SnowGentle>4</SnowGentle>
      <SnowHard>4</SnowHard>
      <GrayPall MayRequire="Ludeon.RimWorld.Anomaly">1</GrayPall>
      <Windy>1</Windy>
      <Overcast>2</Overcast>
    </baseWeatherCommonalities>
    <wildPlants>
      <Plant_Grass>7.0</Plant_Grass>
      <Plant_GrayGrass MayRequire="Ludeon.RimWorld.Biotech">5</Plant_GrayGrass>
      <Plant_Dandelion>0.2</Plant_Dandelion>
      <Plant_Bush>0.7</Plant_Bush>
      <Plant_PincushionCactus>0.2</Plant_PincushionCactus>
      <Plant_MagmaCactus>1.0</Plant_MagmaCactus>
      <Plant_Agave>0.05</Plant_Agave>
      <Plant_TreeDrago>0.2</Plant_TreeDrago>
      <Plant_Berry>0.025</Plant_Berry>
      <BurnedTree>0.1</BurnedTree>
      <Plant_PebbleCactus MayRequire="Ludeon.RimWorld.Biotech">0.6</Plant_PebbleCactus>
      <Plant_Reeds>1</Plant_Reeds>
    </wildPlants>
    <wildAnimals>
      <Donkey>0.2</Donkey>
      <Horse>0.1</Horse>
      <Rat>1.0</Rat>
      <Hare>1.0</Hare>
      <Iguana>0.7</Iguana>
      <Gazelle>0.7</Gazelle>
      <Emu>0.7</Emu>
      <Boomrat>0.5</Boomrat>
      <Boomalope>0.5</Boomalope>
      <Cougar>0.15</Cougar>
      <Fox_Fennec>0.15</Fox_Fennec>
      <GuineaPig>0.015</GuineaPig>
      <MonitorLizard>0.5</MonitorLizard>
      <Vulture>0.5</Vulture>
      <LavaSnail>0.5</LavaSnail>
    </wildAnimals>
    <pollutionWildAnimals MayRequire="Ludeon.RimWorld.Biotech">
      <Toxalope MayRequire="Ludeon.RimWorld.Biotech">0.1</Toxalope>
      <WasteRat MayRequire="Ludeon.RimWorld.Biotech">0.1</WasteRat>
      <Iguana>1</Iguana>
      <Rat>0.6</Rat>
      <Boomalope>0.4</Boomalope>
      <Boomrat>0.4</Boomrat>
      <Megascarab>0.2</Megascarab>
    </pollutionWildAnimals>
    <coastalWildAnimals>
      <HermitCrab>0.1</HermitCrab>
      <StoneCrab>0.1</StoneCrab>
      <SeaTurtle>0.1</SeaTurtle>
    </coastalWildAnimals>
    <allowedPackAnimals>
      <li>Muffalo</li>
      <li>Dromedary</li>
      <li>Elephant</li>
      <li>Alpaca</li>
    </allowedPackAnimals>
  </BiomeDef>

</Defs>