<?xml version="1.0" encoding="utf-8"?><Defs>
  
  <SoundDef>
    <defName>Ambient_SpaceMap</defName>
    <sustain>True</sustain>
    <context>MapOnly</context>
    <priorityMode>PrioritizeNearest</priorityMode>
    <subSounds>
      <li>
        <name>Main</name>
        <onCamera>True</onCamera>
        <grains>
          <li Class="AudioGrain_Clip">
            <clipPath>Ambience/Amb_Space</clipPath>
          </li>
        </grains>
        <volumeRange>3.5~3.5</volumeRange>
        <sustainLoopDurationRange>36~36</sustainLoopDurationRange>
        <sustainIntervalRange>-0.5~-0.5</sustainIntervalRange>
        <sustainAttack>0.5</sustainAttack>
        <sustainRelease>0.5</sustainRelease>
        <paramMappings>
          <li>
            <inParam Class="SoundParamSource_CameraAltitude" />
            <outParam Class="SoundParamTarget_Volume" />
            <paramUpdateMode>Constant</paramUpdateMode>
            <curve>
              <points>
                <li>
                  <loc>
                    <x>37.59342</x>
                    <y>-0.008144975</y>
                  </loc>
                </li>
                <li>
                  <loc>
                    <x>50.79147</x>
                    <y>0.3525248</y>
                  </loc>
                </li>
                <li>
                  <loc>
                    <x>64.42703</x>
                    <y>0.9864293</y>
                  </loc>
                </li>
              </points>
            </curve>
          </li>
          <li>
            <inParam Class="SoundParamSource_AmbientVolume" />
            <outParam Class="SoundParamTarget_Volume" />
          </li>
          <li>
            <inParam Class="SoundParamSource_Underground" />
            <outParam Class="SoundParamTarget_Volume" />
            <paramUpdateMode>Constant</paramUpdateMode>
            <curve>
              <points>
                <li>
                  <loc>
                    <x>0</x>
                    <y>1</y>
                  </loc>
                </li>
                <li>
                  <loc>
                    <x>1</x>
                    <y>0</y>
                  </loc>
                </li>
              </points>
            </curve>
          </li>
        </paramMappings>
      </li>
    </subSounds>
  </SoundDef>

  <SoundDef>
    <defName>Ambient_TorrentialRain</defName>
    <sustain>True</sustain>
    <context>MapOnly</context>
    <priorityMode>PrioritizeNearest</priorityMode>
    <subSounds>
      <li>
        <onCamera>True</onCamera>
        <grains>
          <li Class="AudioGrain_Clip">
            <clipPath>Ambience/RainTorrential</clipPath>
          </li>
        </grains>
        <volumeRange>12</volumeRange>
        <paramMappings>
          <li>
            <inParam Class="SoundParamSource_CameraAltitude" />
            <outParam Class="SoundParamTarget_Volume" />
            <paramUpdateMode>Constant</paramUpdateMode>
            <curve>
              <points>
                <li>
                  <loc>
                    <x>1</x>
                    <y>1</y>
                  </loc>
                </li>
                <li>
                  <loc>
                    <x>33.34967</x>
                    <y>0.9631265</y>
                  </loc>
                </li>
                <li>
                  <loc>
                    <x>69.43261</x>
                    <y>0.5882397</y>
                  </loc>
                </li>
              </points>
            </curve>
          </li>
          <li>
            <inParam Class="SoundParamSource_External">
              <inParamName>LerpFactor</inParamName>
              <defaultValue>0</defaultValue>
            </inParam>
            <outParam Class="SoundParamTarget_Volume" />
            <paramUpdateMode>Constant</paramUpdateMode>
            <curve>
              <points>
                <li>
                  <loc>
                    <x>0</x>
                    <y>0</y>
                  </loc>
                </li>
                <li>
                  <loc>
                    <x>1</x>
                    <y>1</y>
                  </loc>
                </li>
              </points>
            </curve>
          </li>
          <li>
            <inParam Class="SoundParamSource_AmbientVolume" />
            <outParam Class="SoundParamTarget_Volume" />
          </li>
        </paramMappings>
      </li>
    </subSounds>
  </SoundDef>
  
</Defs>