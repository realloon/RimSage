  <SoundDef>
    <defName>Pawn_HiveQueen_Call</defName>
    <context>MapOnly</context>
    <subSounds>
      <li>
        <grains>
          <li Class="AudioGrain_Folder">
            <clipFolderPath>Pawn/Insect/HiveQueen/Call</clipFolderPath>
