<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <LayoutRoomDef>
    <defName>AncientBarracks</defName>
    <sketchResolverDef>AncientBarracks</sketchResolverDef>
    <floorTypes Inherit="false">
      <li>AncientWoodPlankFloor</li>
      <li>CarpetGreyDark</li>
      <li>CarpetSandstone</li>
      <li>CarpetGreenMuddy</li>
    </floorTypes>
    <edgeTerrain>AncientTile</edgeTerrain>
  </LayoutRoomDef>

  <LayoutRoomDef>
    <defName>AncientStorageRoom</defName>
    <sketchResolverDef>AncientStorageRoom</sketchResolverDef>
    <floorTypes Inherit="false">
      <li>AncientTile</li>
      <li>CarpetGreyDark</li>
      <li>CarpetSandstone</li>
      <li>CarpetGreenMuddy</li>
    </floorTypes>
  </LayoutRoomDef>

  <LayoutRoomDef>
    <defName>AncientEquipmentRoom</defName>
    <canMergeWithAdjacentRoom>true</canMergeWithAdjacentRoom>
    <prefabs>
      <AncientStorageCylinder_Edge>
        <countPerTenEdgeCells>3</countPerTenEdgeCells>
        <minMaxRange>5~10</minMaxRange>
      </AncientStorageCylinder_Edge>
      <AncientMachine_Rows>
        <countPerHundredCells>5</countPerHundredCells>
        <minMaxRange>2~3</minMaxRange>
      </AncientMachine_Rows>
      <AncientSystemRacks_Rows>
        <countPerHundredCells>5</countPerHundredCells>
        <minMaxRange>1~10</minMaxRange>
      </AncientSystemRacks_Rows>
    </prefabs>
    <floorTypes>
      <li>AncientTile</li>
    </floorTypes>
  </LayoutRoomDef>

  <LayoutRoomDef>
    <defName>AncientPowerRoom</defName>
    <canMergeWithAdjacentRoom>true</canMergeWithAdjacentRoom>
    <minSingleRectWidth>10</minSingleRectWidth>
    <minSingleRectHeight>10</minSingleRectHeight>
    <prefabs>
      <AncientGenerator_SmallCluster>
        <countPerHundredCells>2</countPerHundredCells>
        <minMaxRange>1~2</minMaxRange>
      </AncientGenerator_SmallCluster>
      <AncientBarrel_Cluster>
        <countPerHundredCells>5</countPerHundredCells>
        <minMaxRange>1~5</minMaxRange>
      </AncientBarrel_Cluster>
    </prefabs>
    <floorTypes>
      <li>AncientConcrete</li>
    </floorTypes>
  </LayoutRoomDef>

  <LayoutRoomDef>
    <defName>AncientLab</defName>
    <minSingleRectWidth>9</minSingleRectWidth>
    <minSingleRectHeight>9</minSingleRectHeight>
    <prefabs>
      <AncientDisplayBank_Edge>
        <countPerTenEdgeCells>3</countPerTenEdgeCells>
        <minMaxRange>2~6</minMaxRange>
      </AncientDisplayBank_Edge>
      <OperatingTables>
        <countPerHundredCells>10</countPerHundredCells>
        <minMaxRange>0~1</minMaxRange>
      </OperatingTables>
    </prefabs>
    <floorTypes>
      <li>SterileTile</li>
    </floorTypes>
  </LayoutRoomDef>

  <LayoutRoomDef>
    <defName>AncientArmory</defName>
    <canMergeWithAdjacentRoom>true</canMergeWithAdjacentRoom>
    <prefabs>
      <AncientLockerBank_Row>
        <countPerTenEdgeCells>3</countPerTenEdgeCells>
        <minMaxRange>5~15</minMaxRange>
      </AncientLockerBank_Row>
      <Shelves_Rows>
        <countPerHundredCells>2</countPerHundredCells>
        <minMaxRange>0~1</minMaxRange>
      </Shelves_Rows>
      <AncientEquipmentBlocks_Rows>
        <countPerHundredCells>2</countPerHundredCells>
        <minMaxRange>1~2</minMaxRange>
      </AncientEquipmentBlocks_Rows>
      <AncientBarrel_Cluster>
        <countPerHundredCells>2</countPerHundredCells>
        <minMaxRange>1~5</minMaxRange>
      </AncientBarrel_Cluster>
    </prefabs>
    <floorTypes>
      <li>AncientTile</li>
    </floorTypes>
  </LayoutRoomDef>

</Defs>